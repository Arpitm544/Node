pipeline
{
    agent any
        stages
        {
            stage("Install dependencies")
            {
                steps
                {
                    sh 'npm install'
                }
            
        }
      stage("Test")
      {
        steps{
                sh 'npm test || echo "No tests defined"'
        }
      }
      stage("Build")
      {
        steps{
                 sh 'npm run build' 
        }
      }

    }
}
